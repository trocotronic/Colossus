INCLUDES = ../include/ircd.h ../include/ircsprintf.h ../include/md5.h \
	../include/modulos.h ../include/protocolos.h ../include/parseconf.h \
	../include/struct.h
R_MODULES=unreal.so p10.so redhispana.so
COMMANDS=unreal.o p10.o redhispana.o
MODULES=$(R_MODULES)
MODULEFLAGS=@MODULEFLAGS@
all: build
build: $(MODULES)
custommodule: $(MODULEFILE).c
	$(CC) $(CFLAGS) $(MODULEFLAGS) -o $(MODULEFILE).so $(MODULEFILE).c 
unreal.so: unreal.c $(INCLUDES)
	$(CC) $(CFLAGS) $(MODULEFLAGS) -o unreal.so unreal.c
p10.so: p10.c $(INCLUDES)
	$(CC) $(CFLAGS) $(MODULEFLAGS) -o p10.so p10.c
redhispana.so: redhispana.c $(INCLUDES)
	$(CC) $(CFLAGS) $(MODULEFLAGS) -o redhispana.so redhispana.c

clean:
	$(RM) -f *.o *.so *~ core
